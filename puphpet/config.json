	"beanstalkd": {
		"install": 0,
		"settings": {
			"listenaddress": "0.0.0.0",
			"listenport": "13000",
			"maxjobsize": "65535",
			"maxconnections": "1024",
			"binlogdir": "/var/lib/beanstalkd/binlog",
			"binlogfsync": null,
			"binlogsize": "10485760"
