		"node_config": {},
		"naster_config": {},
		"general_config": {}
	},
	"firewall": {
		"setup": 1,
		"rules": {
			"002 accept related established rules": {
				"proto": "all",
				"ctstate": ["RELATED", "ESTABLISHED"],
				"action": "accept"
			},
			"006 nginx-http-https": {
				"ensure": "present",
				"port": [80, 443],
				"proto": "tcp",
				"action": "accept",
				"destination": "%{::ipaddress_eth0}"
			},
			"007 vsftpd": {
				"ensure": "present",
				"port": [21],
				"proto": "tcp",
				"action": "accept",
				"destination": "%{::ipaddress_eth0}"
			},
			"008 mqtt": {
				"ensure": "present",
				"port": [2000],
				"proto": "tcp",
				"action": "accept",
				"src_range": "172.18.29.12-172.18.29.13",
				"destination": "%{::ipaddress_eth1}"
			}
		}
	}
