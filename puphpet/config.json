						"--verbose",
						"--hiera_config /vagrant/puphpet/puppet/hiera.yaml",
						"--parser future"
					]
				}
			},
			"synced_folder": {
				"www-root": {
					"id": "www-root",
					"source": "./",
					"target": "/var/www/phalcon.es",
					"type": "nfs",
					"disabled": false
				},
				"/vagrant": {
					"id": "vagrant",
					"source": ".",
					"target": "/vagrant",
					"type": "nfs",
					"disabled": false
				}
			},
			"usable_port_range": "2200..2250"
		},
		"ssh": {
			"host": null,
			"port": null,
			"private_key_path": null,
			"username": "vagrant",
			"password": null,
			"guest_port": null,
			"keep_alive": true,
			"forward_agent": false,
			"forward_x11": false,
			"shell": "bash -l"
		},
		"vagrant": {
			"host": "detect"
		}
	},
	"server": {
		"packages": [
			"pam-devel",
			"python-devel"
		],
		"dot_files": [
			{
				"bash_aliases": null
			}
		],
		"_prevent_empty": ""
	},
	"mailcatcher": {
		"install": 0,
		"settings": {
			"smtp_ip": "0.0.0.0",
			"smtp_port": 1025,
			"http_ip": "0.0.0.0",
			"http_port": "1080",
			"path": "/usr/local/bin",
			"log": "/var/log/mailcatcher/mailcatcher.log"
		}
	},
	"nginx": {
		"gzip": "on",
		"fpm_socket": "/var/run/php5-fpm.sock",
		"mail": true,
		"confd_purge": true,
		"configtest_enable": true,
		"manage_repo": true,
		"worker_processes": "%{::processorcount}",
		"worker_connections": 1024,
		"http_cfg_append": {
			"gzip_static": "on",
			"gzip_comp_level": "9",
			"gzip_disable": "msie6",
			"gzip_types": "text/plain text/css application/json application/javascript application/x-javascript text/javascript text/xml application/xml application/rss+xml application/atom+xml application/rdf+xml",
			"gzip_proxied": "any",
			"gzip_vary": "on"
		},
		"vhosts": {
			"default": {
				"use_default_location":false,
				"ensure": "present",
				"listen_ip": "%{::ipaddress_eth1}",
				"listen_port": 80,
				"server_name": ["main.phalcon.es", "www.main.phalcon.es"],
				"www_root": "/usr/share/nginx/html",
				"access_log": "/var/log/nginx/default.access.log",
				"error_log": "/var/log/nginx/default.error.log",
				"index_files": ["index.html", "index.htm","index.php"],
				"try_files": ["$request_uri", "$request_uri/", "/index.php$is_args$args"],
				"include_files": ["/etc/nginx/mime.types"],
				"add_header":{
					"X-req":"$request_uri",
					"X-uri":"$uri"
				}
			},
			"phalcon.es": {
				"ensure": "present",
				"use_default_location":false,
				"server_name": ["phalcon.es", "www.phalcon.es"],
				"listen_ip": "%{::ipaddress_eth1}",
				"listen_port": 80,
				"www_root": "/var/www/phalcon.es/public",
				"access_log": "/var/log/nginx/phalcon.es.access.log",
				"error_log": "/var/log/nginx/phalcon.es.error.log",
				"index_files": ["index.php", "index.html", "index.htm"],
				"try_files": ["$request_uri", "$request_uri/", "/index.php?_url=$request_uri"],
				"include_files": ["/etc/nginx/mime.types"]
			}
		},
		"locations": {
			"default-root":{
				"try_files": ["$request_uri", "$request_uri/", "/index.php$is_args$args"],
				"vhost":"default",
				"location_custom_cfg":{
					"gzip":"on"
				}
			},
			"default-php": {
				"ensure": "present",
				"vhost": "default",
				"location": "~* \\.php$",
				"fastcgi": "unix:/var/run/php5-fpm.sock",
				"fastcgi_script": "$document_root$fastcgi_script_name",
				"fastcgi_split_path": "^(.+\\.php)(/.+)$",
				"priority": 502,
				"try_files": ["$uri =404"],
				"location_custom_cfg": {
					"fastcgi_param": "PATH_INFO $fastcgi_path_info",
					"fastcgi_param": "PATH_TRANSLATED $document_root$fastcgi_path_info",
					"fastcgi_connect_timeout": "3m",
					"fastcgi_read_timeout": "3m",
					"fastcgi_send_timeout": "3m",
					"fastcgi_index": "index.php",
					"fastcgi_intercept_errors": "on"
				}
			},
			"default-static": {
				"ensure": "present",
				"vhost": "default",
				"location": "~* \\.(ico|3gp|gif|jpg|cur|svg|jpeg|png|wmv|avi|asf|asx|mpg|mpeg|mp4|woff|pls|mp3|mid|wav|swf|flv|js|html|htm|txt|css|exe|zip|tar|rar|gz|tgz|bz2|uha|7z|doc|docx|xls|xlsx|pdf|iso|htc)(\\?[0-9]+)?$",
				"priority": 501,
				"location_custom_cfg": {
					"expires": "max",
					"access_log": "off",
					"log_not_found": "off",
					"error_log": "off",
					"add_header": "Pragma \"public\""
				}
			},
			"phalcon.es-root":{
				"try_files": ["$request_uri", "$request_uri/", "/index.php$is_args$args"],
				"vhost":"phalcon.es",
				"location_custom_cfg":{
					"gzip":"on"
				}
			},
			"phalcon.es-php": {
				"ensure": "present",
				"vhost": "phalcon.es",
				"location": "~* \\.php$",
				"fastcgi": "unix:/var/run/php5-fpm.sock",
				"fastcgi_script": "$document_root$fastcgi_script_name",
				"fastcgi_split_path": "^(.+\\.php)(/.+)$",
				"priority": 502,
				"try_files": ["$uri =404"],
				"location_custom_cfg": {
					"fastcgi_param": "PATH_INFO $fastcgi_path_info",
					"fastcgi_param": "PATH_TRANSLATED $document_root$fastcgi_path_info",
					"fastcgi_connect_timeout": "3m",
					"fastcgi_read_timeout": "3m",
					"fastcgi_send_timeout": "3m",
					"fastcgi_index": "index.php",
					"fastcgi_intercept_errors": "on"
				}
			},
			"phalcon.es-static": {
				"ensure": "present",
				"vhost": "phalcon.es",
				"location": "~* \\.(ico|3gp|gif|jpg|cur|svg|jpeg|png|wmv|avi|asf|asx|mpg|mpeg|mp4|woff|pls|mp3|mid|wav|swf|flv|js|html|htm|txt|css|exe|zip|tar|rar|gz|tgz|bz2|uha|7z|doc|docx|xls|xlsx|pdf|iso|htc)(\\?[0-9]+)?$",
				"priority": 501,
				"location_custom_cfg": {
					"expires": "max",
					"access_log": "off",
					"log_not_found": "off",
					"error_log": "off",
					"add_header": "Pragma \"public\""
				}
			}
		}
	},
