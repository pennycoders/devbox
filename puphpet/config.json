	},
	"elastic_search": {
		"install": 0
	},
	"vsftpd": {
		"install": 1,
		"template": "vsftpd/vsftpd.conf.erb",
		"manage_users": 1,
		"puppi": true,
		"service_status": true,
		"port": 21,
		"pam_service_name": "vsftpd",
		"firewall": false,
		"protocol": "tcp",
		"anonymous_enable": false,
		"anon_mkdir_write_enable": false,
		"anon_upload_enable": false,
		"chroot_local_user": true,
		"connect_from_port_20": true,
		"ftpd_banner": "Hey there, stranger!",
		"guest_enable": false,
		"local_enable": true,
		"hide_ids": true,
		"users": {
			"phalcon": {
				"ensure": "present",
				"managehome": false,
				"shell": "/sbin/nologin",
				"password": "%{sha1('phalconFTP')}",
				"home": "/var/www/phalcon.es",
				"before": "Nginx::Resource::Vhost[phalcon.es]"
			}
		}
	},
	"memcached": {
		"install": 1,
		"package_ensure": "present",
		"logfile": "/var/log/memcached.log",
		"manage_firewall": false,
		"max_memory": "12%",
		"item_size": false,
		"lock_memory": false,
		"listen_ip": "0.0.0.0",
		"tcp_port": "11211",
		"udp_port": "11211",
		"max_connections": "8192",
		"install_dev": false,
		"processorcount": "%{::processorcount}"
	},
	"mosquitto": {
		"install": 1,
		"listen_ip": "%{::ipaddress_eth1}",
		"listen_port": "2000"
	},
	"redis": {
		"install": 1,
		"bind": "127.0.0.1",
		"port": 6379,
		"service_group": "redis",
		"service_user": "redis",
		"manage_repo": true,
		"service_enable": true,
		"config_owner": "root"
	},
	"phalcon": {
		"install": 1,
		"ensure": "master",
		"ensure_sys_deps": true,
